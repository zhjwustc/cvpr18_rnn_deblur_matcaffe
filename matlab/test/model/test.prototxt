    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 1  
  }
  convolution_param {
    num_output: 16
    kernel_size: 11
    stride: 1
    group : 1
    pad: 5
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relue1_1"
